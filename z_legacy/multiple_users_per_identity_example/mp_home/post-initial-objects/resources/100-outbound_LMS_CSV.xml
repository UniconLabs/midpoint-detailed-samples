<?xml version="1.0" encoding="UTF-8"?>
<resource xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
    xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3" 
    xmlns:icfs="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/resource-schema-3" 
    xmlns:org="http://midpoint.evolveum.com/xml/ns/public/common/org-3" 
    xmlns:q="http://prism.evolveum.com/xml/ns/public/query-3" 
    xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance-3" 
    xmlns:t="http://prism.evolveum.com/xml/ns/public/types-3" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    oid="1d3d7b78-fe39-4713-9479-6964da198ecf" 
    version="1">
    <name>Outbound LMS CSV</name>
    <description>Inbound/Outbound but mostly Outbound CSV for LMS</description>
    <iteration>0</iteration>
    <iterationToken/>
    <connectorRef relation="org:default" type="c:ConnectorType">
        <!-- ConnId com.evolveum.polygon.connector.csv.CsvConnector v2.4 -->
        <description>
            ConnId com.evolveum.polygon.connector.csv.CsvConnector v2.4
        </description>
        <filter>
            <q:equal>
                <q:path>connectorType</q:path>
                <q:value>com.evolveum.polygon.connector.csv.CsvConnector</q:value>
            </q:equal>
        </filter>
    </connectorRef>
    <connectorConfiguration xmlns:icfc="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/connector-schema-3" xsi:type="c:ConnectorConfigurationType">
        <icfc:configurationProperties xmlns:gen358="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/bundle/com.evolveum.polygon.connector-csv/com.evolveum.polygon.connector.csv.CsvConnector">
            <gen358:multivalueDelimiter>;</gen358:multivalueDelimiter>
            <gen358:ignoreEmptyLines>true</gen358:ignoreEmptyLines>
            <gen358:fieldDelimiter>,</gen358:fieldDelimiter>
            <gen358:trim>true</gen358:trim>
            <gen358:headerExists>true</gen358:headerExists>
            <gen358:filePath>/opt/midpoint/var/csv/LMS_Outbound_People.csv</gen358:filePath>
            <gen358:quoteMode>NONE</gen358:quoteMode>
            <gen358:readOnly>false</gen358:readOnly>
            <gen358:multivalueAttributes>AFFILIATIONS</gen358:multivalueAttributes>
            <gen358:ignoreIdentifierCase>true</gen358:ignoreIdentifierCase>
            <gen358:uniqueAttribute>ID</gen358:uniqueAttribute>
            <gen358:passwordAttribute>PASSWORD</gen358:passwordAttribute>
        </icfc:configurationProperties>
    </connectorConfiguration>
    <schema></schema>
    <schemaHandling>
        <objectType id="3">
            <default>true</default>
            <objectClass>ri:AccountObjectClass</objectClass>
            <attribute id="4">
                <ref>ri:ID</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>extension/username</path>
                    </source>
                </outbound>
                <inbound id="5">
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <target>
                        <path>extension/username</path>
                    </target>
                </inbound>
                <inbound id="20">
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <target>
                        <path>name</path>
                    </target>
                </inbound>
            </attribute>
            <attribute id="7">
                <ref>ri:EMAIL</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>emailAddress</path>
                    </source>
                </outbound>
                <inbound id="16">
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <target>
                        <path>emailAddress</path>
                    </target>
                </inbound>
            </attribute>
            <attribute id="8">
                <ref>ri:FIRSTNAME</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>givenName</path>
                    </source>
                </outbound>
                <inbound id="17">
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <target>
                        <path>givenName</path>
                    </target>
                </inbound>
            </attribute>
            <attribute id="9">
                <ref>ri:LASTNAME</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>familyName</path>
                    </source>
                </outbound>
                <inbound id="18">
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <target>
                        <path>familyName</path>
                    </target>
                </inbound>
            </attribute>
            <attribute id="10">
                <ref>ri:BIRTHDATE</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>extension/birthdate</path>
                    </source>
                    <expression>
                        <script>
                            <code>
                        if (birthdate) {
                        return basic.formatDateTime("yyyy-MM-dd HH:mm:ss.S", birthdate)
                        } 
                    </code>
                        </script>
                    </expression>
                </outbound>
                <inbound id="19">
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <expression>
                        <script>
                            <code>
                                    basic.parseDateTime("yyyy-MM-dd HH:mm:ss.S", input)
                                </code>
                        </script>
                    </expression>
                    <target>
                        <path>extension/birthdate</path>
                    </target>
                </inbound>
            </attribute>
            <credentials>
                <password xsi:type="c:ResourcePasswordDefinitionType">
                    <outbound/>
                </password>
            </credentials>
        </objectType>
    </schemaHandling>
    <capabilities>
        <native xmlns:cap="http://midpoint.evolveum.com/xml/ns/public/resource/capabilities-3" xsi:type="c:CapabilityCollectionType">
            <cap:schema/>
            <cap:liveSync/>
            <cap:testConnection/>
            <cap:create/>
            <cap:update>
                <cap:addRemoveAttributeValues>true</cap:addRemoveAttributeValues>
            </cap:update>
            <cap:delete/>
            <cap:script>
                <cap:host>
                    <cap:type>resource</cap:type>
                </cap:host>
                <cap:host>
                    <cap:type>connector</cap:type>
                </cap:host>
            </cap:script>
            <cap:read>
                <cap:returnDefaultAttributesOption>false</cap:returnDefaultAttributesOption>
            </cap:read>
        </native>
    </capabilities>
    <synchronization>
        <objectSynchronization>
            <objectClass>AccountObjectClass</objectClass>
            <kind>account</kind>
            <focusType>c:UserType</focusType>
            <enabled>true</enabled>
            <correlation>
                <q:equal xmlns="">
                    <q:path>extension/username</q:path>
                    <expression xmlns="">
                        <path>
                            $projection/attributes/ID
                        </path>
                    </expression>
                </q:equal>
            </correlation>
            <reconcile>false</reconcile>
            <reaction>
                <situation>linked</situation>
                <synchronize>true</synchronize>
            </reaction>
            <reaction>
                <situation>deleted</situation>
                <synchronize>true</synchronize>
                <action>
                    <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/action-3#unlink</handlerUri>
                </action>
            </reaction>
            <reaction>
                <situation>unlinked</situation>
                <action>
                    <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/action-3#link</handlerUri>
                </action>
            </reaction>
            <reaction>
                <situation>unmatched</situation>
            </reaction>
        </objectSynchronization>
    </synchronization>
</resource>
